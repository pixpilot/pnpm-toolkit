  "packages/private-npm/package.json": "{
  "name": "private-npm",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.17.0",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixpilot/pnpm-toolkit.git",
    "directory": "packages/private-npm"
  },
  "keywords": [],
  "exports": "./src/index.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "clean": "git clean -xdf .cache .turbo dist",
    "clean:all": "git clean -xdf .cache .turbo dist node_modules",
    "build": "pnpm run clean && tsc -p tsconfig.build.json",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore"
  },
  "devDependencies": {
    "@internal/eslint-config": "workspace:*",
    "@internal/prettier-config": "workspace:*",
    "@internal/tsconfig": "workspace:*",
    "@internal/vitest-config": "workspace:*",
    "@types/node": "catalog:dev",
    "eslint": "catalog:dev",
    "typescript": "catalog:dev"
  },
  "prettier": "@internal/prettier-config"
}
",
