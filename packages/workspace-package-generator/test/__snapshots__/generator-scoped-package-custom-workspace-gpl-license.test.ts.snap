export default baseConfig;
",
  "custom/scoped-pkg/package.json": "{
  "name": "@scope/scoped-pkg",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixpilot/pnpm-toolkit.git",
    "directory": "custom/scoped-pkg"
  },
  "keywords": [],
  "exports": "./src/index.ts",
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist",
    "clean:all": "git clean -xdf .cache .turbo dist node_modules",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore"
  },
  "devDependencies": {
    "@internal/eslint-config": "workspace:*",
    "@internal/prettier-config": "workspace:*",
    "@internal/tsconfig": "workspace:*",
    "@internal/vitest-config": "workspace:*",
    "@types/node": "catalog:dev",
    "eslint": "catalog:dev",
    "typescript": "catalog:dev"
  },
  "prettier": "@internal/prettier-config"
}
",
  "custom/scoped-pkg/src/index.ts": "export const name = '@scope/scoped-pkg';",
  "custom/scoped-pkg/test/index.test.ts": "import { describe, expect, it } from 'vitest';

import { name } from '../src';

describe('name', () => {
  it('should be defined', () => {
    expect(name).toBeDefined();
  });
});
",
  "custom/scoped-pkg/tsconfig.json": "{
  "extends": "@internal/tsconfig/base.json",
  "include": ["src/**/*", "test/**/*", "scripts/**/*", "*.config.ts", "*.config.js"]
}
",
  "custom/scoped-pkg/vitest.config.ts": "import baseConfig from '@internal/vitest-config';

export default baseConfig;
",
}